{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2017",

        "noImplicitAny": false,
        "incremental": true,
        "removeComments": false,
        "noUnusedLocals": false,
        "noImplicitThis": true,
        "inlineSourceMap": false,
        "sourceMap": true,
        "outDir": "../out",
        "preserveConstEnums": true,
        "strictNullChecks": true,
        "noUnusedParameters": false,
        "esModuleInterop": true
    }
}
